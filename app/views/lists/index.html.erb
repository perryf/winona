<h2>Lists <%= link_to "(+)", new_user_list_path(current_user) %></h2>

<ul>
  <% current_user.lists.each do |list| %>
    <li>
      <%= link_to list.name, user_list_path(current_user, list) %>
      (<%= list.movies.count %>)
    </li>
  <% end %>
</ul>

<div class="go-back">
  <%= link_to "Go Back", :back %>
</div>
