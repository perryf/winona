<div class="error-message">
  <% @list.errors.full_messages.each do |message| %>
    <p><%= message %></p>
  <% end %>
</div>

<%= form_for @list do |f| %>
  <div class="">
    <%= f.label :name, "Name:" %>
    <%= f.text_field :name %>
  </div>

  <div class="">
    <%= f.submit %>
  </div>
<% end %>
